  <script>
    const divOne = document.getElementById("divOne") as HTMLElement;
    const divTwo = document.getElementById("divTwo") as HTMLElement;
    const divThree = document.getElementById("divThree") as HTMLElement;

    const buttonOne = document.getElementById("buttonOne");
    const buttonTwo = document.getElementById("buttonTwo");
    const buttonThree = document.getElementById("buttonThree");

    const divs = [divOne, divTwo, divThree];

    function showDiv(activeDiv: HTMLElement) {
      const active = activeDiv.dataset.active!;
      const inactive = activeDiv.dataset.inactive!;

      divs.forEach((div) => {
        div.classList.remove(active, inactive);
        div.classList.add(div === activeDiv ? active : inactive);
      });
    }

    buttonOne?.addEventListener("click", () => showDiv(divOne));
    buttonTwo?.addEventListener("click", () => showDiv(divTwo));
    buttonThree?.addEventListener("click", () => showDiv(divThree));
  </script>
