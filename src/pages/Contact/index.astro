            <a
              href="https://mail.google.com/mail/?view=cm&fs=1&to=diego.gp227@gmail.com"
              class={style.textRed}>diego.gp227@gmail.com</a
            >
          </div>
        </div>
      </div>

      <div class={style.formDiv}>
        <form action="" class={style.form}>
          <label for="" class={style.labelForm}>Nombre</label>
          <input
            type="text"
            class={`${style.inputForm} ${style.inputOne}`}
            autofocus
          />
          <label for="" class={style.labelForm}>Asunto</label>
          <input type="email" class={`${style.inputForm} ${style.inputTwo}`} />
          <label for="Text" class={style.labelForm}>Texto</label>
          <textarea name="Text" class={`${style.inputForm} ${style.inputTree}`}
          ></textarea>
          <button class={style.button}
            >Enviar!

            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="24"
              height="24"
              viewBox="0 0 24 24"
              ><g fill="none"
                ><path
                  d="m12.594 23.258l-.012.002l-.071.035l-.02.004l-.014-.004l-.071-.036q-.016-.004-.024.006l-.004.01l-.017.428l.005.02l.01.013l.104.074l.015.004l.012-.004l.104-.074l.012-.016l.004-.017l-.017-.427q-.004-.016-.016-.018m.264-.113l-.014.002l-.184.093l-.01.01l-.003.011l.018.43l.005.012l.008.008l.201.092q.019.005.029-.008l.004-.014l-.034-.614q-.005-.019-.02-.022m-.715.002a.02.02 0 0 0-.027.006l-.006.014l-.034.614q.001.018.017.024l.015-.002l.201-.093l.01-.008l.003-.011l.018-.43l-.003-.012l-.01-.01z"
